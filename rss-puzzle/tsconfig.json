{
  "compilerOptions": {
    "target": "es2016",
    "module": "Node16",
    "moduleResolution": "Node16",
    "baseUrl": "./",
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
